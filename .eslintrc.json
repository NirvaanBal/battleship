{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-unneeded-ternary": "off",
        "no-param-reassign": "off",
        "operator-linebreak": "off",
        "comma-dangle": "off"
    }
}
